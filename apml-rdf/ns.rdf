<!-- APML-RDF Ontology -->

<owl:Ontology rdf:about="http://rdfs.org/apml-rdf/ns#">
  <dc:title xml:lang="en">APML-RDF Ontology Namespace</dc:title>
  <owl:versionInfo>Revision: 0.01</owl:versionInfo>
  <dc:description xml:lang="en">APML-RDF (Attention Profiling Markup Language) is an RDF ontology of the APML open standard for encapsulating a summary of your interests (across multiple profiles) in a simple, portable way.</dc:description>
  <rdfs:seeAlso rdf:resource="http://apml.pbwiki.com/" rdfs:label="APML Specification"/>
</owl:Ontology>

